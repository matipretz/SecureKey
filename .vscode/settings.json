{
    "[git-commit]": {
        "editor.rulers": [50]
    },    
    "[python]": {
        "editor.glyphMargin": true,//muestra u oculta debug steps
        "editor.formatOnType": true,
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.rulers":[79,120]
    },

    "security.workspace.trust.untrustedFiles": "open",
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "workbench.sideBar.location": "right",
    "workbench.editor.showTabs": true,//muestra u oculta pesta√±as de editores abiertos
    "java.inlayHints.parameterNames.enabled": "none",//tipos de parametros esperados
    "explorer.confirmDelete": false,
    "editor.lightbulb.enabled": false,
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "editor.overviewRulerBorder": false,
    "editor.guides.indentation": false,
    "breadcrumbs.enabled": true,//muestra u oculta la ruta de navegacion
    "indenticator.width": 1,
    "indenticator.color.dark": "rgba(255,255,255,0.1)",
    "editor.minimap.enabled": false,//muestra u oculta el minimapa de codigo
    "editor.matchBrackets": "never",
    "editor.glyphMargin": true,//muestra u oculta debug steps
    "redhat.telemetry.enabled": false,
    "workbench.startupEditor": "none",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "editor.linkedEditing": true,
    "workbench.iconTheme": "vs-seti" ,
    "workbench.colorTheme": "GitHub Dark Default",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.guides.bracketPairs": true,
    "workbench.colorCustomizations": {
        "[GitHub Dark Default]": {}
    
    },
    "settingsSync.ignoredExtensions": [

    ],
